	pages = {518--528},
	file = {Mellor et al. - 2016 - Semisupervised Gaussian Process for Automated Enzy.pdf:/home/saheru/Zotero/storage/E4BY7G4Q/Mellor et al. - 2016 - Semisupervised Gaussian Process for Automated Enzy.pdf:application/pdf},
}

