	keywords = {Feature extraction, AM-PseAAC, Chemicals, DP-PseAAC, drug-target interactions, Drugs, molecular substructure fingerprint, over-sampling SMOTE, Predictive models, Protein sequence, PSSM-Bigram, Standards, XGBoost classifier},
